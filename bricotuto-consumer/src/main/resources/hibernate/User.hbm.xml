<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.asf.bricotuto.model.bean.User.User" table="user">
		<id name="userId" type="int" column="user_id">
			<generator class="increment" />
		</id>
		<property name="login" type="string" not-null="true" />
		<property name="password" type="string" not-null="true" />
		<property name="inscriptionDt" type="date" column="inscription_dt" not-null="true" />
		<property name="updateDt" type="date" column="update_dt" />
	</class>
</hibernate-mapping>

